  "dependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "prismjs": "^1.28.0",
    "vant": "^3.5.4",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-md": "^0.20.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.7",
    "vue-tsc": "^0.39.5"
  }
}
